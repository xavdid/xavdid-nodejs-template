{
  "version": "0.0.0",
  "name": "xavdid-node-template",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "test": "jest",
    "validate": "yarn test && yarn lint",
    "release": "npx np"
  },
  "keywords": [],
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://xavd.id"
  },
  "license": "ISC",
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@xavdid/eslint-config": "^1.0.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  }
}
